+  ow(input, ow.string.nonEmpty.label('input'))
+  ow(nthFrame, ow.number.integer)
+  ow(numSteps, ow.number.integer.positive.label('numSteps'))
+  if (output) ow(output, ow.string.nonEmpty.label('output'))
